{
	"name": "next-app",
	"private": true,
	"scripts": {
		"build": "node ./scripts/build && next export && mv out/_next out/next && sed -i '' -e 's/\\/_next/\\.\\/next/g' out/**.html && mv out/index.html ../extension && rsync -va --delete-after out/next/ ../extension/next/ && rm -rf out && rsync -va --delete-after public/assets ../extension/",
		"next": "node ./scripts/dev",
		"start": "node ./scripts/build && node ./scripts/start",
		"git:unused": "next-unused",
		"tailwind": "tailwindcss -i ./design/styles/input.css -o ./design/styles/output.css --watch",
		"translate": "i18next --config lang.config.js"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@headlessui/react": "^1.6.3",
		"@heroicons/react": "^1.0.6",
		"@magic-ext/oauth": "^2.1.1",
		"@magic-sdk/admin": "^1.4.1",
		"@mobiscroll/react": "file:./scripts/build/mobiscroll-react-v5.15.2.tgz",
		"@prisma/client": "^3.14.0",
		"@userlist/push": "^0.2.0",
		"date-fns": "^2.28.0",
		"i18next": "^21.8.4",
		"magic-sdk": "^8.1.1",
		"next": "latest",
		"pluralize": "^8.0.0",
		"react": "latest",
		"react-dom": "latest",
		"react-i18next": "^11.16.9",
		"swr": "2.0.0-beta.3",
		"tinydate": "^1.3.0"
	},
	"devDependencies": {
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/typography": "^0.5.2",
		"autoprefixer": "^10.4.7",
		"inquirer": "^8.2.4",
		"next-unused": "^0.0.6",
		"postcss": "^8.4.14",
		"prisma": "^3.14.0",
		"shelljs": "^0.8.5",
		"tailwindcss": "^3.0.24"
	},
	"next-unused": {
		"alias": {
			"+": "apis",
			"$": "content",
			"-": "contexts",
			"~": "design/elements",
			"^": "design/layouts",
			"@": "design/styles",
			"=": "hooks",
			"%": "lib"
		},
		"include": [
			"apis",
			"design",
			"hooks",
			"pages"
		],
		"entrypoints": [
			"pages"
		]
	}
}
