{
	"name": "simpler | prioritize your life",
	"description": "simpler is a productivity app that helps you prioritize what needs to get done so you can focus on what truly matters.",
	"version": "2.0.2",
	"manifest_version": 3,
	"action": {
		"default_title": "simpler | prioritize your life",
		"default_popup": "index.html"
	},
	"content_security_policy": {
		"extension_pages": "script-src 'self'; object-src 'self'"
	},
	"icons": {
		"72": "/assets/icons/app-icon-72.png",
		"96": "/assets/icons/app-icon-96.png",
		"128": "/assets/icons/app-icon-128.png",
		"144": "/assets/icons/app-icon-144.png",
		"152": "/assets/icons/app-icon-152.png",
		"192": "/assets/icons/app-icon-192.png",
		"512": "/assets/icons/app-icon-512.png",
		"1024": "/assets/icons/app-icon-1024.png"
	}
}
